package Com.Akhila;
class std
{
	String name = "Akhila";   
    int rollno = 03;
    
    std(){
    System.out.println("default constructor");
    }
}
public class todayy {
      
    public static void main(String[] args) {
        
         std s1 = new std();
         System.out.println("Name: " + s1.name);
         System.out.println("rollno: " + s1.rollno);
    }

}  


OUTPUT:
default constructor
Name: Akhila
rollno: 03
